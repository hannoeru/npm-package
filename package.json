{
  "name": "npm-package",
  "description": "My npm package template",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "hannoeru <me@hanlee.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hannoeru/npm-package"
  },
  "homepage": "https://github.com/hannoeru/npm-package",
  "bugs": "https://github.com/hannoeru/npm-package/issues",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp --push --tag --commit && npm publish",
    "example:dev": "npm -C example run dev",
    "example:build": "npm -C example run build"
  },
  "dependencies": {
    "debug": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "eslint": "^7.21.0",
    "rollup": "^2.40.0",
    "tsup": "^4.6.1",
    "typescript": "^4.2.3"
  }
}
